{
  "paths": {
    "dev_csv": "data/raw/development.csv",
    "eval_csv": "data/raw/evaluation.csv",
    "cv_out_dir": "reports/cv",
    "model_out": "models/model.pkl",
    "submission_out": "models/submission.csv"
  },
  "cv": { "k": 5, "seed": 42 },
  "text": {
    "max_features": 100000,
    "ngram_min": 1,
    "ngram_max": 2,
    "min_df": 5,
    "title_repeat": 2,
    "missing_article_token": "__MISSING_ARTICLE__",
    "max_df": 0.95,
    "lowercase": true,
    "strip_accents": "unicode",
    "sublinear_tf": true,
    "stop_words": null,
    "char_enabled": true,
    "char_analyzer": "char_wb",
    "char_ngram_min": 3,
    "char_ngram_max": 5,
    "char_min_df": 3,
    "char_max_features": null,
    "title_char": false,
    "title_char_ngram_min": 2,
    "title_char_ngram_max": 4,
    "title_char_min_df": 4,
    "title_char_max_features": 20000
  },
  "model": {
    "type": "linearsvc",
    "C": 8.0,
    "max_iter": 10000,
    "class_weight": "balanced",
    "class_weight_power": 0.7,
    "logreg_solver": "liblinear",
    "logreg_n_jobs": 1,
    "linearsvc_dual": false,
    "ridge_alpha": 1.0
  },
  "source": {
    "min_count": 3,
    "min_frac": null
  },
  "dedup": {
    "drop_same_label": false
  },
  "leakage": {
    "drop_overlap": false
  }
}
